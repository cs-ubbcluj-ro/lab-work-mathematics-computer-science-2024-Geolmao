number n; 
read n;
number x = 0;
number i = 2;
bool prime ;

while (x < n) {
    prime = true;
    j = 2;
    while ( j <= sqrt(i) ) {
        if (i%j == 0) {
            prime = false;
            j = sqrt(i) + 1;
        }
        j = j + 1;
    }
    if(prime){
        write(i);
        write(' ');
        x = x + 1;
    }
    i = i+1;
}
write('\n');
